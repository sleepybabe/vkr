{
	"name":  "Extension_checkcriteria", 
	"version": "1.0", 
    "manifest_version": 3,
    
    "action": {
        "default_popup":"popup.html"
    },

    "permissions": ["scripting", "activeTab", "tabs"],
    "host_permissions": ["<all_urls>"],
    
    "background": {
        "service_worker": "background.js"
    },

    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["override.js"]
        }
    ]
}
  